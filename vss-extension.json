{
    "manifestVersion": 1,
    "id": "cyberark-conjur-secrets",
    "name": "Cyberark Conjur Secrets",
    "version": "0.0.16",
    "publisher": "InfamousJoeG",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tool for retrieving secrets from Cyberark Conjur Secrets Manager",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/cyberark/conjur-azure-devops-extension/blob/master/overview.md"
        },
        "issues": {
            "uri": "https://github.com/cyberark/conjur-azure-devops-extension/issues"
        },
        "repository": {
            "uri": "https://github.com/cyberark/conjur-azure-devops-extension"
        }
	},
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "conjurTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "conjurTask"
            }
        }
    ]
}
